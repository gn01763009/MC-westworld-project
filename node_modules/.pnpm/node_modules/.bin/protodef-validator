#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/reggie/reggie_coding_space/mineflayer/node_modules/.pnpm/protodef-validator@1.4.0/node_modules/protodef-validator/node_modules:/Users/reggie/reggie_coding_space/mineflayer/node_modules/.pnpm/protodef-validator@1.4.0/node_modules:/Users/reggie/reggie_coding_space/mineflayer/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/reggie/reggie_coding_space/mineflayer/node_modules/.pnpm/protodef-validator@1.4.0/node_modules/protodef-validator/node_modules:/Users/reggie/reggie_coding_space/mineflayer/node_modules/.pnpm/protodef-validator@1.4.0/node_modules:/Users/reggie/reggie_coding_space/mineflayer/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../protodef-validator/cli.js" "$@"
else
  exec node  "$basedir/../protodef-validator/cli.js" "$@"
fi
